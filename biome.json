{
	"$schema": "https://biomejs.dev/schemas/1.8.2/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noExplicitAny": "off"
			},
			"performance": {
				"noDelete": "off"
			},
			"complexity": {
				"noStaticOnlyClass": "off"
			}
		}
	}
}
